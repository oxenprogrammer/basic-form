<%= form_with model: @user do |form| %>
  <%= form.label :username, :value => 'Your user name here' %>:
  <%= form.text_field :username %><br><br>
  <%= form.label :email %>:
  <%= form.text_field :email, :value => 'example@example.com' %><br><br>
  <%= form.label :password %>:
  <%= form.password_field :password %><br><br>
  <%= form.submit  %>
<% end %>
